                <el-table-column prop="Column23" width="150" label="加工时间（天）"></el-table-column>
                <el-table-column prop="Column24" width="150" label="进度"></el-table-column>
            </el-table-column>
            <el-table-column prop="Column25" width="150" label="实际加工完成时间"></el-table-column>
            <el-table-column label="组装">
                <el-table-column prop="Column26" width="150" label="组装（%）"></el-table-column>
                <el-table-column prop="Column27" width="150" label="调试（%）"></el-table-column>
                <el-table-column prop="Column28" width="150" label="进度说明"></el-table-column>
            </el-table-column>
            <el-table-column prop="Column29" width="150" label="备注"></el-table-column>
            <el-table-column prop="Column30" width="150" label="部门负责人"></el-table-column>
            <el-table-column prop="Column31" width="150" label="记录人"></el-table-column>
            @*<el-table-column prop="Column32" width="150" label="设计员确认收货"></el-table-column>*@
            <el-table-column prop="Column33" width="150" label="备注"></el-table-column>
            <el-table-column prop="Column34" width="150" label="图纸审核员"></el-table-column>
        </el-table>
        <!--底部翻頁-->
        <div style="text-align:right">
            <el-pagination v-on:size-change="handleSizeChange"
                           v-on:current-change="handleCurrentChange"
                           :current-page="currentPage"
                           :page-sizes="[1, 3, 5, 10]"
                           :page-size="10000"
                           layout="total, sizes, prev, pager, next, jumper"
                           :total="totalRows">
            </el-pagination>
        </div>
        @*<el-table :data="tableData" stripe border>
            <el-table-column prop="CodeNo" type="index"></el-table-column>
