            <el-table-column prop="Column5" width="150" label="项目编码"></el-table-column>
            <el-table-column prop="Column6" width="150" label="项目名称"></el-table-column>
            <el-table-column prop="Column7" width="150" label="流水号"></el-table-column>
            <el-table-column prop="Column8" width="150" label="bom"></el-table-column>
            <el-table-column prop="Column9" width="150" label="走账"></el-table-column>
            <el-table-column prop="Column10" width="150" label="设计员"></el-table-column>
